{
  "name": "redeemify-backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.ts",
  "scripts": {
    "start": "nodemon ./src/main.ts",
    "install-sequelize": "npm i -g sequelize sequelize-cli",
    "start-dev": "set NODE_ENV=development&&set port=4000&& nodemon ./src/main.ts",
    "build:local": "npm run install-sequelize && npm run migrate && npm run start ",
    "build-dev": "npm run install-sequelize && npm run migrate-dev && npm run start ",
    "start-test": "set NODE_ENV=test&&set port=5000&& nodemon ./src/main.ts",
    "start-prod": "set NODE_ENV=production&&set port=6000&& nodemon ./src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-migrate": "sequelize-cli migration:generate --config ./config/config.json --env localhost --name ",
    "create-migrate-dev": "sequelize-cli migration:generate --config ./config/config.json --env development --name ",
    "create-migrate-test": "sequelize-cli migration:generate --config ./config/config.json --env test --name ",
    "create-migrate-prod": "sequelize-cli migration:generate --config ./config/config.json --env production --name ",
    "migrate": "sequelize-cli db:migrate --config ./config/config.json --env localhost",
    "migrate-dev": "sequelize-cli db:migrate --config ./config/config.json --env development",
    "migrate-test": "sequelize-cli db:migrate --config ./config/config.json --env test",
    "migrate-prod": "sequelize-cli db:migrate --config ./config/config.json --env production",
    "generate-seed": "sequelize-cli seed:generate  --config ./config/config.json --env localhost --name ",
    "generate-seed-dev": "sequelize-cli seed:generate  --config ./config/config.json --env development --name ",
    "generate-seed-test": "sequelize-cli seed:generate  --config ./config/config.json --env test --name ",
    "generate-seed-prod": "sequelize-cli seed:generate  --config ./config/config.json --env production --name ",
    "execute-seeds": "sequelize-cli db:seed:all  --config ./config/config.json --env localhost",
    "execute-seeds-dev": "sequelize-cli db:seed:all  --config ./config/config.json --env development",
    "execute-seeds-test": "sequelize-cli db:seed:all  --config ./config/config.json --env test",
    "execute-seeds-prod": "sequelize-cli db:seed:all  --config ./config/config.json --env production",
    "undo-migrate": "sequelize-cli db:migrate:undo:all --config ./config/config.json --env localhost",
    "undo-migrate-dev": "sequelize-cli db:migrate:undo:all --config ./config/config.json --env development",
    "undo-seeds-test": "sequelize-cli db:seed:undo:all --config ./config/config.json --env test",
    "undo-seeds-production": "sequelize-cli db:seed:undo:all --config ./config/config.json --env production",
    "execute-seed": " sequelize-cli db:seed  --config ./config/config.json --env localhost --seed ",
    "execute-seed-dev": " sequelize-cli db:seed  --config ./config/config.json --env development --seed ",
    "execute-seed-test": " sequelize-cli db:seed  --config ./config/config.json --env test --seed ",
    "execute-seed-prod": " sequelize-cli db:seed  --config ./config/config.json --env prod --seed ",
    "undo-seed": "sequelize-cli db:seed:undo --config ./config/config.json --env localhost  --seed ",
    "undo-seed-dev": "sequelize-cli db:seed:undo --config ./config/config.json --env development  --seed ",
    "undo-seed-test": "sequelize-cli db:seed:undo --config ./config/config.json --env test  --seed ",
    "undo-seed-prod": "sequelize-cli db:seed:undo --config ./config/config.json --env production  --seed ",
    "build:development": "webpack --config webpack.config.js",
    "build:production": "cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/multer": "^1.4.8",
    "@types/multer-s3": "^3.0.1",
    "@types/pg": "^8.10.2",
    "@types/sharp": "^0.32.0",
    "aws-sdk": "^2.1468.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "2.10.0",
    "pg": "^8.11.3",
    "sharp": "^0.32.6",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.1.0",
    "@types/cls-hooked": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/db-migrate-base": "^0.0.10",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "argon2": "^0.31.0",
    "axios": "^1.5.1",
    "blob": "^0.1.0",
    "body-parser": "^1.20.2",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^3.0.1",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^11.5.3",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "webpack-node-externals": "^3.0.0"
  }
}
